pipeline {
   agent any
   stages {
       stage('Build Code') {
           steps {
               sh "mvn clean package"
               echo "Building Artifact for project"

               
           }
       }
       stage('Reading branch wise')
       {
       when
       {
       branch "master*"
       }
       steps
       {
       echo " It is only for master branch"
       }
       }

       stage('Deploy Code') {
	   
          steps {
               sh "mvn tomcat11:deploy"
               echo "Deploying Code"
               
          }
      }
      }
      }

